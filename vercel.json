{
  "version": 2,
  "projectSettings": {
    "nodeVersion": "18.x"
  },
  "buildCommand": "cd client && npm install && npm run build",
  "installCommand": "npm install",
  "env": {
    "VITE_SERVER_URL": "@vite_server_url"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
